#include "uuids.as"

void main(Scene@ scene)
{
  scene.loadSceneLayer(uuids::cornellBox_Empty_SceneLayer);
  scene.loadSceneLayer(uuids::cornellBox_LightTest_RectSizes_SceneLayer);
  scene.loadSceneLayer(uuids::cornellBox_RoughnessTestMirrors_SceneLayer);
  scene.addSceneLayer_debugCamera();
  
  scene.camera[CameraSlot::MAIN_CAMERA] = uuids::cornellBox_cameraMirrors;
}
