void main(ResourceIndex@ index)
{
  array<string> meshesToImport = {".*"};
  dictionary uuids;

  convertSceneGraph(sceneGraphFile: "cornell-box.scene-graph",
                    sourceFile: "cornell-box.blend",
                    meshesToImport: meshesToImport,
                    meshUuids: uuids,
                    materialUuids: uuids,
                    lightUuids: uuids);
}
