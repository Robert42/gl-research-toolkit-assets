void main(ResourceIndex@ index)
{
  SceneGraphImportSettings sceneImportSettings;
  array<string> meshesToImport = {".*"};
  dictionary uuids;
  
  sceneImportSettings.meshesToImport = meshesToImport;
  sceneImportSettings.meshUuids = uuids;
  sceneImportSettings.materialUuids = uuids;
  sceneImportSettings.lightUuids = uuids;

  convertSceneGraph(sceneGraphFile: "cornell-box.scene-graph",
                    sourceFile: "cornell-box.blend",
                    uuid: Uuid<ResourceGroup>("{336a9321-fb5f-4566-9c1c-4a88832bda48}"),
                    settings: sceneImportSettings);
}
